{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/shared-ui",
  "lib": {
    "entryFile": "src/public-api.ts"
  },
  "assets": [
    {
      "input": "./src/config",
      "glob": "**/*.mjs",
      "output": "config"
    },
    {
      "input": "./src/config",
      "glob": "**/*.css",
      "output": "config"
    }
  ],
  "deleteDestPath": false
}
